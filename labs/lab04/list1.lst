     1                                  ; lab4.asm
     2                                  SECTION .data ; Начало секции данных
     3 00000000 D090D182D0B0D0BDD0-     familiya: DB 'Атанесов Даниил',10 ; 'Атанесов Даниил' плюс
     3 00000009 B5D181D0BED0B220D0-
     3 00000012 94D0B0D0BDD0B8D0B8-
     3 0000001B D0BB0A             
     4                                  ; символ перевода строки
     5                                  familiyaLen: EQU $-familiya ; Длина строки hello
     6                                  SECTION .text ; Начало секции кода
     7                                  GLOBAL _start
     8                                  _start: ; Точка входа в программу
     9 00000000 B804000000              mov eax,4 ; Системный вызов для записи (sys_write)
    10 00000005 BB01000000              mov ebx,1 ; Описатель файла '1' - стандартный вывод
    11 0000000A B9[00000000]            mov ecx,familiya ; Адрес строки hello в ecx
    12 0000000F BA1E000000              mov edx,familiyaLen ; Размер строки hello
    13 00000014 CD80                    int 80h ; Вызов ядра
    14 00000016 B801000000              mov eax,1 ; Системный вызов для выхода (sys_exit)
    15 0000001B BB00000000              mov ebx,0 ; Выход с кодом возврата '0' (без ошибок)
    16 00000020 CD80                    int 80h ; Вызов ядра
